{
    "version": "2.1",
    "title": "Calibre-Web",
    "name": "Calibre-Web",
    "icon": "https://github.com/ArevomEisvolk/AppStore/blob/main/Apps/Calibre-Web/icon.png?raw=true",
    "tagline": "Calibre-web - ein schickes Webfrontend für eure E-books",
    "overview": "Calibre-web ist eine ansehnliche web-basierte App, mit denen ihr eure E-Book Sammlung verwalten könnt.",
    "thumbnail": "https://raw.githubusercontent.com/linuxserver/docker-templates/master/linuxserver.io/img/calibre-web-icon.png",
    "screenshots": [],
    "category": [
        "Media",
        "Books"
    ],
    "developer": {
        "name": "Calibre-Web",
        "website": "https://github.com/janeczku/calibre-web"
    },
    "container": {
        "image": "linuxserver/calibre-web:latest",
        "shell": "bash",
        "privileged": false,
        "network_model": "bridge",
        "web_ui": {
            "http": "8083",
            "path": "/"
        },
        "envs": [
            {
                "key": "UID",
                "value": "1000"
            },
            {
                "key": "GID",
                "value": "100"
            },
            {
                "key": "GIDLIST",
                "value": "100"
            }
        ],
        "ports": [
            {
                "container": "8083",
                "host": "8083"
            }
        ],
        "volumes": [
            {
                "container": "/config",
                "host": "/DATA/AppData/$AppID/config"
            },
            {
                "container": "/books",
                "host": "/DATA/books"
            }
        ],
        "devices": [],
        "constraints": {
            "min_memory": 512,
            "min_storage": 2048
        },
        "restart_policy": "unless-stopped",
        "sysctls": [],
        "cap_add": {},
        "labels": []
    },
    "tips": {}
}